import java.io.*;
public class Quiz
{
    static BufferedReader q=new BufferedReader(new InputStreamReader(System.in));
    static void sports() throws IOException
    {
        int score=0;
        System.out.println();
        System.out.println(" Which Indian Cricketer is popularly called 'Jammy' ?");
        System.out.println("1. Sachin Tendulkar");
        System.out.println("2. Saurav Ganguly");
        System.out.println("3. Yuvraj Singh");
        System.out.println("4. Rahul Dravid");
        System.out.println("Enter your choice: ");
        String q1=q.readLine();
        int a1=Integer.parseInt(q1);  
        System.out.println();
        if(a1==4)
        {
            System.out.println("      CORRECT ANSWER");
            score+=30;
        }
        else
        {
            System.out.println("     SORRY! BETTER LUCK NEXT TIME.THE CORRECT ANSWER IS RAHUL DRAVID.");
        }
        System.out.println("Your Score: "+score);
        System.out.println();
        System.out.println(" Who is the present manager of Manchester United F.C. ?");
        System.out.println("1. Alex Ferguson");
        System.out.println("2. José Mourinho");
        System.out.println("3. Louis van Gaal");
        System.out.println("4. David Moyes");
        System.out.println("Enter your choice: ");
        String q2=q.readLine();
        int a2=Integer.parseInt(q2);  
        System.out.println();
        if(a2==2)
        {
            System.out.println("      CORRECT ANSWER");
            score+=30;
        }
        else
        {
            System.out.println("     SORRY! BETTER LUCK NEXT TIME.THE CORRECT ANSWER IS JOSE MOURINHO.");
        }
        System.out.println("Your Score: "+score);
        System.out.println();
        System.out.println(" The Olympic gold metal consists of 5.2% of which metal ?");
        System.out.println("1. Gold");
        System.out.println("2. Platinum");
        System.out.println("3. Silver");
        System.out.println("4. Copper");
        System.out.println("Enter your choice: ");
        String q3=q.readLine();
        int a3=Integer.parseInt(q3);  
        System.out.println();
        if(a3==1)
        {
            System.out.println("      CORRECT ANSWER");
            score+=30;
        }
        else
        {
            System.out.println("     SORRY! BETTER LUCK NEXT TIME.THE CORRECT ANSWER IS GOLD.");
        }
        System.out.println("Your Score: "+score);
        System.out.println();
        System.out.println(" The standard colours of which sporting event are green & purple ?");
        System.out.println("1. Asian Cup");
        System.out.println("2. Euro Cup");
        System.out.println("3. Wimbledon");
        System.out.println("4. Commonwealth Games");
        System.out.println("Enter your choice: ");
        String q4=q.readLine();
        int a4=Integer.parseInt(q4);  
        System.out.println();
        if(a4==3)
        {
            System.out.println("      CORRECT ANSWER");
            score+=30;
        }
        else
        {
            System.out.println("     SORRY! BETTER LUCK NEXT TIME.THE CORRECT ANSWER IS WIMBLEDON.");
        }
        System.out.println("Your Score: "+score);
        System.out.println();
        System.out.println(" How many players are there in the game of basketball ?");
        System.out.println("1. 5");
        System.out.println("2. 6");
        System.out.println("3. 8");
        System.out.println("4. 11");
        System.out.println("Enter your choice: ");
        String q5=q.readLine();
        int a5=Integer.parseInt(q5);  
        System.out.println();
        if(a5==2)
        {
            System.out.println("      CORRECT ANSWER");
            score+=30;
        }
        else
        {
            System.out.println("     SORRY! BETTER LUCK NEXT TIME.THE CORRECT ANSWER IS 6.");
        }
        System.out.println("Your Score: "+score);    
        System.out.println();
        System.out.println("You have finally scored: "+score+" points.");
        System.out.println();
        if(score<=90)
        {
            System.out.println("SORRY! WE DON'T HAVE A PRIZE FOR YOU");
        }
        else
        {
            System.out.println("CONGRATULATIONS! YOU WIN A LIFE LONG TRIP TO YOUR HOME.LUCKY GUY.");
        }
    }

